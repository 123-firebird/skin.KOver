<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<include name="HOMViewtype">
	
		<control type="group">
			<visible>Control.IsVisible($PARAM[Id])</visible>	

			<!-- Background -->
			<control type="image">
				<texture background="true">$INFO[Container($PARAM[Id]).ListItem.Art(fanart)]</texture>
				<aspectratio>scale</aspectratio>
				<visible>[!Player.HasVideo | !Skin.String($PARAM[CompPrefix].WPVDOTop,VISIBLE)] + Skin.String($PARAM[CompPrefix].WPBgs,FANART) + !IsEmpty(Container($PARAM[Id]).ListItem.Art(fanart))</visible>
			</control>
			<control type="image">
				<aspectratio>scale</aspectratio>
				<texture background="true">$INFO[Container(9000).ListItem.Property(background)]</texture>
				<visible>[!Player.HasVideo | !Skin.String($PARAM[CompPrefix].WPVDOTop,VISIBLE)] + !Control.HasFocus($PARAM[Id]) + !IsEmpty(Container(9000).ListItem.Property(background))</visible>
			</control>


			<control type="group">
				<animation effect="slide" start="0,0" end="0,120" time="200" condition="StringCompare(Skin.String(HOM.View1.BBSidebar),BOTTOM) + [Skin.String($PARAM[CompPrefix].BBCoordY,C) | [Skin.String($PARAM[CompPrefix].BBCoordY,B) + !Skin.String($PARAM[CompPrefix].BBSizeY,1ROW)]] + Control.HasFocus(9000) + IsEmpty(Window(Home).Property(DialogVisibility)) + !Skin.String($PARAM[CompPrefix].BBSizeY,3ROW)">Conditional</animation>
				<animation effect="slide" start="0,0" end="0,-120" time="200" condition="StringCompare(Skin.String(HOM.View1.BBSidebar),TOP) + Skin.String($PARAM[CompPrefix].BBCoordY,A) + !Skin.String($PARAM[CompPrefix].BBSizeY,3ROW) + Skin.String($PARAM[CompPrefix].BBCoordY,A) + Control.HasFocus(9000) + IsEmpty(Window(Home).Property(DialogVisibility)) + !Skin.String($PARAM[CompPrefix].BBSizeY,3ROW)">Conditional</animation>
				<animation effect="slide" start="0,0" end="-120,0" time="200" condition="StringCompare(Skin.String(HOM.View1.BBSidebar),LEFT) + Skin.String($PARAM[CompPrefix].BBCoordX,A) + !Skin.String($PARAM[CompPrefix].BBSizeX,3COLUMN) + Skin.String($PARAM[CompPrefix].BBCoordX,A) + Control.HasFocus(9000) + IsEmpty(Window(Home).Property(DialogVisibility)) + !Skin.String($PARAM[CompPrefix].BBSizeX,3COLUMN)">Conditional</animation>
				<animation effect="slide" start="0,0" end="120,0" time="200" condition="StringCompare(Skin.String(HOM.View1.BBSidebar),RIGHT) + [Skin.String($PARAM[CompPrefix].BBCoordX,C) | [Skin.String($PARAM[CompPrefix].BBCoordX,B) + !Skin.String($PARAM[CompPrefix].BBSizeX,1COLUMN)]] + Skin.String($PARAM[CompPrefix].BBCoordX,C) + Control.HasFocus(9000) + IsEmpty(Window(Home).Property(DialogVisibility)) + !Skin.String($PARAM[CompPrefix].BBSizeX,3COLUMN)">Conditional</animation>
			 
				<include name="DialogPush">
					<param name="CompPrefix" value="$PARAM[CompPrefix]" />
				</include>
			
				<control type="image">
					<include name="BgPosition">
						<param name="Prefix" value="$PARAM[CompPrefix]" />
					</include>
					<texture colordiffuse="$INFO[Skin.String(Color.BBBg)]">common/white.png</texture>
					<animation effect="fade" start="100" end="0" time="400" reversible="false" condition="System.IdleTime(3) + Skin.String($PARAM[CompPrefix].BBShow,IDLEFADE)">Conditonal</animation>
					<visible>!Skin.String($PARAM[CompPrefix].BBShow,HIDDEN)</visible>
				</control>				

				<!-- Bloc 4 / special background -->
				<control type="group">
					<include name="BlocPosition">
						<param name="Prefix" value="$PARAM[CompPrefix]" />
						<param name="BlocID" value="B4" />
					</include>	
					<control type="image">
						<left>0</left>
						<top>0</top>
						<texture colordiffuse="$INFO[Skin.String(Color.B4Bg)]">common/white.png</texture>
					</control>
					<visible>!Skin.String($PARAM[CompPrefix].B4Show,HIDDEN)</visible>
					<animation effect="fade" start="100" end="0" time="200" condition="[Window.IsVisible(contextmenu) | Control.HasFocus(9001)] + Skin.String($PARAM[CompPrefix].B5B4ShowOF,HIDE)">Conditional</animation>
				</control>
				
				<!-- Bloc 3 -->
				<control type="group">
					<animation effect="fade" start="100" end="0" time="400" reversible="false" condition="System.IdleTime(3) + Skin.String($PARAM[CompPrefix].B3Show,IDLEFADE)">Conditonal</animation>
					<animation effect="fade" start="100" end="0" time="200" condition="[Window.IsVisible(contextmenu) | Control.HasFocus(9001)] + Skin.String($PARAM[CompPrefix].B5B3ShowOF,HIDE)">Conditional</animation>
					<include name="BlocPosition">
						<param name="Prefix" value="$PARAM[CompPrefix]" />
						<param name="BlocID" value="B3" />
					</include>
					<visible>!Skin.String($PARAM[CompPrefix].B3Show,HIDDEN)</visible>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<texture colordiffuse="$INFO[Skin.String(Color.B3Bg)]">common/white.png</texture>
					</control>				

					<!-- Episode / Film preview -->
					<control type="group">
						<control type="image">
							<left>0</left>
							<top>0</top>
							<align>left</align>
							<aligny>top</aligny>
							<aspectratio>scale</aspectratio>
							<texture background="true" colordiffuse="00FFFFFF">$INFO[Weather.Conditions]</texture>
						</control>
						<control type="image">
							<left>0</left>
							<top>0</top>
							<align>left</align>
							<aligny>top</aligny>
							<aspectratio>scale</aspectratio>
							<texture background="true" colordiffuse="FFFFFFFF">$VAR[B3Art]</texture>
						</control>
						<control type="image">
							<left>0</left>
							<top>0</top>
							<align>left</align>
							<aligny>top</aligny>
							<aspectratio>scale</aspectratio>
							<texture background="true" colordiffuse="FFFFFFFF">$INFO[Container($PARAM[Id]).ListItem.Art(fanart)]</texture>
							<visible>!StringCompare(Container(9000).ListItem.Property(widgetType),episodes)</visible>
						</control>
						<control type="image">
							<left>0</left>
							<top>0</top>
							<align>left</align>
							<aligny>top</aligny>
							<aspectratio>scale</aspectratio>
							<texture background="true" colordiffuse="FFFFFFFF">$INFO[Container($PARAM[Id]).ListItem.Art(thumb)]</texture>
							<visible>StringCompare(Container(9000).ListItem.Property(widgetType),episodes) | StringCompare(Container(9000).ListItem.Property(widgetType),weather)</visible>
						</control>
						<control type="image">
							<left>0</left>
							<top>0</top>
							<align>left</align>
							<aligny>top</aligny>
							<aspectratio>scale</aspectratio>
							<texture background="true">http://localhost:52307/getpvrthumb&amp;title=$INFO[Container($PARAM[Id]).ListItem.Title]&amp;channel=$INFO[Container($PARAM[Id]).ListItem.ChannelName]&amp;type=fanart,thumb</texture>
							<visible>!IsEmpty(Container($PARAM[Id]).ListItem.ChannelName) + !IsEmpty(Container($PARAM[Id]).ListItem.Title)</visible>
						</control>
						<control type="group">
							<control type="group">
								<right>0</right>
								<top>0</top>
								<width>100</width>
								<height>100</height>
								<control type="image">
									<right>0</right>
									<top>0</top>
									<aspectratio>scale</aspectratio>
									<texture colordiffuse="FFFFFFFF">common/corner.png</texture>
								</control>
								<control type="image">
									<right>5</right>
									<top>5</top>
									<width>25</width>
									<height>25</height>
									<aspectratio>scale</aspectratio>
									<texture colordiffuse="FFFFFFFF">material/check.png</texture>
								</control>
							</control>
							<control type="group">
								<visible>!IsEmpty(Container($PARAM[Id]).ListItem.Tagline) | !IsEmpty(Container($PARAM[Id]).ListItem.TVShowTitle) | !IsEmpty(Container($PARAM[Id]).ListItem.Artist)</visible>
								<include name="B3STag">
									<param name="Prefix" value="$PARAM[CompPrefix]" />
								</include>
								<control type="image">
									<left>0</left>
									<top>300</top>
									<height>56</height>
									<aspectratio aligny="bottom">stretch</aspectratio>
									<texture colordiffuse="AA000000">common/white.png</texture>
								</control>
								<control type="label">
									<left>12</left>
									<top>300</top>
									<width>589</width>
									<height>56</height>
									<align>left</align>
									<textcolor>FFFFFFFF</textcolor>
									<font>FontSmall</font>
									<label>$INFO[Container($PARAM[Id]).ListItem.Tagline]</label>
									<visible>StringCompare(Container(9000).ListItem.Property(widgetType),movies)</visible>
								</control>
								<control type="label">
									<left>12</left>
									<top>300</top>
									<width>589</width>
									<height>56</height>
									<align>left</align>
									<textcolor>FFFFFFFF</textcolor>
									<font>FontSmall</font>
									<label>$INFO[Container($PARAM[Id]).ListItem.TVShowTitle,, . ]$INFO[Container($PARAM[Id]).ListItem.Season,$LOCALIZE[20373] , . ]$INFO[Container($PARAM[Id]).ListItem.Episode,$LOCALIZE[20359] ,]</label>
									<visible>StringCompare(Container(9000).ListItem.Property(widgetType),episodes)</visible>
								</control>
								<control type="label">
									<left>12</left>
									<top>300</top>
									<width>589</width>
									<height>56</height>
									<align>left</align>
									<textcolor>FFFFFFFF</textcolor>
									<font>FontSmall</font>
									<label>$INFO[Container($PARAM[Id]).ListItem.Year,(,) ]$INFO[Container($PARAM[Id]).ListItem.Artist]</label>
									<visible>StringCompare(Container(9000).ListItem.Property(widgetTarget),music)</visible>
								</control>
							</control>
						</control>
					</control>
				</control>		
				
				<!-- Bloc 4 -->
				<control type="group">
					<include name="BlocPosition">
						<param name="Prefix" value="$PARAM[CompPrefix]" />
						<param name="BlocID" value="B4" />
					</include>	
					<control type="image">
						<left>0</left>
						<top>0</top>
						<texture colordiffuse="$INFO[Skin.String(Color.B4Bg)]">common/white.png</texture>
					</control>				
					<control type="image">
						<left>0</left>
						<top>0</top>
						<texture colordiffuse="FFFFFFFF">$INFO[Container(9000).ListItem.Property(homeart)]</texture>
					</control>
					<control type="image">
						<left>0</left>
						<top>0</top>
						<align>left</align>
						<aligny>top</aligny>
						<aspectratio>scale</aspectratio>
						<texture background="true">http://localhost:52307/getpvrthumb&amp;title=$INFO[Container($PARAM[Id]).ListItem.Title]&amp;channel=$INFO[Container($PARAM[Id]).ListItem.ChannelName]&amp;type=fanart,thumb</texture>
						<visible>!IsEmpty(Container($PARAM[Id]).ListItem.ChannelName) + !IsEmpty(Container($PARAM[Id]).ListItem.Title)</visible>
					</control>
 					<control type="image"> 
 						<left>0</left> 
 						<top>0</top> 
 						<height>118</height> 
 						<aspectratio>stretch</aspectratio> 
						<texture colordiffuse="AA000000">common/white.png</texture> 
 					</control>
 					<control type="label"> 
						<top>10</top>
						<height>60</height>
						<right>24</right> 
						<align>right</align> 
						<font>Font36R</font> 
 						<textcolor>FFFFFFFF</textcolor> 
 						<label>$INFO[Container(9000).ListItem.Label]</label> 
 					</control> 
 					<control type="label"> 
						<top>45</top>
						<height>60</height>
 						<right>24</right> 
						<align>right</align> 
 						<font>FontSmallB</font> 
						<textcolor>FFFFFFFF</textcolor> 
						<label fallback="1034">$INFO[Container(9000).ListItem.Property(widgetName)]</label>
					</control>

					<visible>!Skin.String($PARAM[CompPrefix].B4Show,HIDDEN)</visible>
					<animation effect="fade" start="100" end="0" time="400" reversible="false" condition="System.IdleTime(3) + Skin.String($PARAM[CompPrefix].B4Show,IDLEFADE)">Conditonal</animation>
					<animation effect="fade" start="100" end="0" time="200" condition="[Window.IsVisible(contextmenu) | Control.HasFocus(9001)] + Skin.String($PARAM[CompPrefix].B5B4ShowOF,HIDE)">Conditional</animation>
				</control>		

				<!-- Bloc 1 -->	
				<control type="group">
					<visible>Control.IsVisible($PARAM[Id])</visible>
					<animation effect="fade" start="100" end="0" time="0" condition="Skin.String($PARAM[CompPrefix].B1Show,HIDDEN)">Conditional</animation>
					<animation effect="fade" start="100" end="0" time="400" reversible="false" condition="System.IdleTime(3) + Skin.String($PARAM[CompPrefix].B1Show,IDLEFADE)">Conditonal</animation>
					<animation effect="fade" start="100" end="0" time="200" condition="[Window.IsVisible(contextmenu) | Control.HasFocus(9001)] + Skin.String($PARAM[CompPrefix].B5B1ShowOF,HIDE)">Conditional</animation>
					<include name="BlocPosition">
						<param name="Prefix" value="$PARAM[CompPrefix]" />
						<param name="BlocID" value="B1" />
					</include>

					<control type="image">
						<left>0</left>
						<top>0</top>
						<texture colordiffuse="$INFO[Skin.String(Color.B1Bg)]">common/white.png</texture>
					</control>	

					<!-- Main menu -->	
					<control type="panel" id="$PARAM[Id]">

						<left>0</left>
						<top>0</top>

						<include condition="Skin.String($PARAM[CompPrefix].B1Lay,HORIZONTAL)">SetHorizontal</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Lay,VERTICAL)">SetVertical</include>

						<onup condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONUP)">9001</onup>
						<onup condition="Skin.String($PARAM[CompPrefix].B1Goback,ONUP)">9000</onup>

						<ondown condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONDOWN)">9001</ondown>
						<ondown condition="Skin.String($PARAM[CompPrefix].B1Goback,ONDOWN)">9000</ondown>

						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + IsEmpty(Window(Home).Property(TVShowNext))">9000</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + !IsEmpty(Window(Home).Property(TVShowNext))">ClearProperty(TVShowNext,home)</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + !StringCompare(Container(9000).ListItem.Property(widgetType),pvr)">9000</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONLEFT)">9001</onleft>

						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + !IsEmpty(Window(Home).Property(TVShowNext))">9001</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + IsEmpty(Window(Home).Property(TVShowNext))">SetProperty(TVShowNext,NEXT,home)</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + !StringCompare(Container(9000).ListItem.Property(widgetType),pvr)">9001</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Goback,ONRIGHT)">9000</onright>
						<onback>9000</onback>
						
						<scrolltime>200</scrolltime>
						<autoscroll time="8000">System.IdleTime(3) + Control.HasFocus(9000)</autoscroll>
						<viewtype label="$INFO[Skin.String($PARAM[CompPrefix].Name)]">wrap</viewtype>
						<preloaditems>2</preloaditems>
						<visible allowhiddenfocus="true">!StringCompare(Container(9000).ListItem.Property(widgetType),weather)</visible>

						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,1COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="636" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,2COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="1272" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,3COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="1908" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,TILES)" name="ListTiles">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,FANART)" name="ListFanart">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,CARD)" name="ListCard">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,POSTER)" name="ListPoster">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,FIXEDLIST) | Skin.String($PARAM[CompPrefix].B1Typ,BANNER)" name="ListFixed">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<content target="$PARAM[Target]">$PARAM[Path]</content>
					</control>	

					<!-- Main menu -->	
					<control type="panel" id="$PARAM[Id]">

						<left>0</left>
						<top>0</top>

						<include condition="Skin.String($PARAM[CompPrefix].B1Lay,HORIZONTAL)">SetHorizontal</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Lay,VERTICAL)">SetVertical</include>

						<onup condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONUP)">9001</onup>
						<onup condition="Skin.String($PARAM[CompPrefix].B1Goback,ONUP)">9000</onup>

						<ondown condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONDOWN)">9001</ondown>
						<ondown condition="Skin.String($PARAM[CompPrefix].B1Goback,ONDOWN)">9000</ondown>

						<onleft condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONLEFT)">9001</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + !IsEmpty(Window(Home).Property(TVShowNext))">ClearProperty(TVShowNext,home)</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + IsEmpty(Window(Home).Property(TVShowNext))">9000</onleft>
						<onleft condition="Skin.String($PARAM[CompPrefix].B1Goback,ONLEFT) + !StringCompare(Container(9000).ListItem.Property(widgetType),pvr)">9000</onleft>

						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + IsEmpty(Window(Home).Property(TVShowNext))">SetProperty(TVShowNext,NEXT,home)</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST) + StringCompare(Container(9000).ListItem.Property(widgetType),pvr) + !IsEmpty(Window(Home).Property(TVShowNext))">9001</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Submenu,ONRIGHT) + !StringCompare(Container(9000).ListItem.Property(widgetType),pvr)">9001</onright>
						<onright condition="Skin.String($PARAM[CompPrefix].B1Goback,ONRIGHT)">9000</onright>
						<onback>9000</onback>
						
						<scrolltime>200</scrolltime>
						<autoscroll time="8000">System.IdleTime(3) + Control.HasFocus(9000)</autoscroll>
						<viewtype label="$INFO[Skin.String($PARAM[CompPrefix].Name)]">wrap</viewtype>
						<preloaditems>2</preloaditems>
						<visible allowhiddenfocus="true">StringCompare(Container(9000).ListItem.Property(widgetType),weather)</visible>

						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,1COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="636" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,2COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="1272" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include name="ListWide" condition="Skin.String($PARAM[CompPrefix].B1SizeX,3COLUMN) + Skin.String($PARAM[CompPrefix].B1Typ,WIDELIST)">
							<param name="width" value="1908" />
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,TILES)" name="ListTiles">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,FANART)" name="ListFanart">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,CARD)" name="ListCard">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
						</include>
						<include condition="Skin.String($PARAM[CompPrefix].B1Typ,POSTER)" name="ListPoster">
							<param name="CompPrefix" value="$PARAM[CompPrefix]" />
